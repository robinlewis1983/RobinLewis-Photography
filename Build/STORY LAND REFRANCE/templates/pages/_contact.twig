

{# You can omit the `.twig` when referencing another template: #}
{% extends '_layout' %}

{# Provide something to the `content` block in `_layout.twig`: #}
{% block content %}
  
  {% set clientBlocks = entry.pageContent.all() %}


  <div class="mediaTextDiv">

  {% for clientBlockPrint in clientBlocks %}


              {% include 'includes/cp.mediatext.twig' with {
                  mediahHeader: clientBlockPrint.contactHeader,
                  mediaContentText: clientBlockPrint.contactBlurb,
                  mediaContentImg: clientBlockPrint.contactImage,
                  mediaEmail: clientBlockPrint.email,
                  mediaPhoneNumber: clientBlockPrint.phoneNumber,
                  customClass: 'mediaText--OneModule', 
              } %}
                
  {% endfor %}

  </div>

{% endblock %}
